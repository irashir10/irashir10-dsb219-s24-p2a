      "source": [
        "def hamming_distance(seq1, seq2):\n",
        "    if len(seq1) != len(seq2):\n",
        "        raise ValueError(\"not equal length\")\n",
        "        #'nope'\n",
        "    distance = 0\n",
        "    for i in range(len(seq1)):\n",
        "        if seq1[i] != seq2[i]:\n",
        "            distance += 1\n",
        "    return distance"
      ],
      "metadata": {
        "id": "Zmzo9NUEa2nA"
      },
      "execution_count": 16,
      "outputs": []
